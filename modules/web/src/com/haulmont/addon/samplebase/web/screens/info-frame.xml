<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.haulmont.addon.samplebase.web.screens.InfoFrame"
        messagesPack="com.haulmont.addon.samplebase.web.screens">
    <dialogMode height="600"
                width="800"/>
    <layout>
        <hbox expand="infoLab_vbox"
              margin="true,false,false,true"
              spacing="true"
              stylename="info-box"
              width="100%">
            <label align="MIDDLE_CENTER"
                   icon="font-icon:INFO_CIRCLE"
                   stylename="info-icon"/>
            <vbox id="infoLab_vbox"
                  expand="infoLab">
                <label id="infoLab"
                       value="README not found"
                       width="100%"/>
                <flowBox id="linksBox"
                         height="35px"
                         spacing="true"/>
            </vbox>
        </hbox>
    </layout>
</window>
